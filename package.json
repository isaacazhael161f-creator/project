{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 expo start",
    "web": "expo start --web",
    "build:web": "expo export --platform web",
    "build:web:prod": "cross-env NODE_ENV=production expo export --platform web --output-dir dist-web",
    "build:analyze": "cross-env ANALYZE_BUNDLE=true npm run build:web:prod",
    "preview:web": "npx serve dist-web -s",
    "lint": "expo lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "node scripts/run-integration-tests.js",
    "test:integration:watch": "jest --watch --testPathPattern=\".*\\.integration\\.test\\.(ts|tsx)$\"",
    "test:e2e": "node scripts/run-integration-tests.js --suite \"End-to-End Integration Tests\"",
    "test:browser": "node scripts/run-integration-tests.js --suite \"Browser Compatibility Tests\"",
    "test:mobile": "node scripts/run-integration-tests.js --suite \"Mobile/Desktop Compatibility Tests\"",
    "test:accuracy": "node scripts/run-integration-tests.js --suite \"Accuracy Validation Tests\"",
    "test:performance": "node scripts/run-integration-tests.js --suite \"Performance Integration Tests\"",
    "test:all": "npm run test && npm run test:integration",
    "deploy:build": "npm run build:web:prod",
    "deploy:preview": "npm run build:web:prod && npm run preview:web",
    "deploy:staging": "node scripts/deploy.js staging",
    "deploy:production": "node scripts/deploy.js production",
    "deploy:check": "node scripts/pre-deploy-checks.js"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.1",
    "@expo/vector-icons": "^14.1.0",
    "@expo/webpack-config": "^19.0.1",
    "@lucide/lab": "^0.1.2",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/drawer": "^7.3.9",
    "@react-navigation/native": "^7.0.14",
    "date-fns": "^4.1.0",
    "expo": "53.0.20",
    "expo-blur": "~14.1.5",
    "expo-build-properties": "~0.14.8",
    "expo-camera": "~16.1.11",
    "expo-constants": "~17.1.3",
    "expo-document-picker": "^13.1.6",
    "expo-file-system": "^18.1.11",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-local-authentication": "^16.0.5",
    "expo-notifications": "^0.31.4",
    "expo-router": "~5.1.4",
    "expo-secure-store": "^14.2.3",
    "expo-sharing": "^13.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-sqlite": "^15.2.14",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "lucide-react-native": "^0.475.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "tesseract.js": "^6.0.1",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.0",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "babel-preset-expo": "~13.0.0",
    "cross-env": "^7.0.3",
    "jest": "~29.7.0",
    "jest-expo": "^53.0.9",
    "react-test-renderer": "^19.0.0",
    "serve": "^14.2.4",
    "ts-jest": "^29.4.1",
    "typescript": "~5.8.3",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
